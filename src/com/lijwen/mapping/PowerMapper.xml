<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lijwen.dao.PowerMapper" >
  <resultMap id="BaseResultMap" type="com.lijwen.po.Power" >
    <constructor >
      <arg column="powerId" jdbcType="INTEGER" javaType="java.lang.Integer" />
      <arg column="powerName" jdbcType="VARCHAR" javaType="java.lang.String" />
    </constructor>
  </resultMap>
  <insert id="insert" parameterType="com.lijwen.po.Power" >
    <selectKey resultType="java.lang.Integer" keyProperty="powerid" order="AFTER" >
      SELECT LAST_INSERT_ID()
    </selectKey>
    insert into power (powerName)
    values (#{powername,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.lijwen.po.Power" >
    <selectKey resultType="java.lang.Integer" keyProperty="powerid" order="AFTER" >
      SELECT LAST_INSERT_ID()
    </selectKey>
    insert into power
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="powername != null" >
        powerName,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="powername != null" >
        #{powername,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>